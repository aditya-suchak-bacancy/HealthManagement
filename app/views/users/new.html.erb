<h2 class="title">SignUp</h2>
<% unless @user.errors.empty? %> 
    <div class="alert alert-danger" role="alert">
    <% @user.errors.full_messages.each do |message| %>
        <div><%= message %></div>
    <% end %> 
    </div> 
<% end %>
<%= render 'form' %>
</br>
<%= link_to "Already a user? Login" ,new_session_path,class:"linkstyle" %>